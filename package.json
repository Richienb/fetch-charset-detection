{
    "name": "fetch-charset-detection",
    "description": "Convert a buffer and headers to UTF-8 text, originally from node-fetch.",
    "keywords": [
        "content-type",
        "headers",
        "http",
        "meta",
        "node-fetch"
    ],
    "version": "0.0.0",
    "main": "dist/index.js",
    "files": [
        "src/**/*",
        "dist/**/*"
    ],
    "repository": "https://github.com/Richienb/fetch-charset-detection.git",
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev": "microbundle --target node --watch",
        "build": "yarn js && yarn docs",
        "js": "microbundle --target node --format es,cjs --external http,https,stream,zlib,cheerio,content-type,iconv-lite",
        "docs": "typedoc --out ./docs --mode file --target ES6 --ignoreCompilerErrors ./src",
        "lint": "eslint src/*"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.3",
        "content-type": "^1.0.4",
        "iconv-lite": "^0.5.0"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.13",
        "@types/content-type": "^1.1.3",
        "@typescript-eslint/eslint-plugin": "^2.6.0",
        "@typescript-eslint/parser": "^2.3.1",
        "eslint": "^6.4.0",
        "eslint-config-google": "^0.14.0",
        "microbundle": "^0.11.0",
        "typedoc": "^0.15.0"
    },
    "resolutions": {
        "microbundle/rollup-plugin-typescript2/rollup-pluginutils/micromatch/braces": "^2.3.1",
        "microbundle/rollup-plugin-postcss/cssnano/postcss-svgo/svgo/js-yaml": "^3.13.0"
    }
}
